@model WasteNotBE.Models.Item
@using Microsoft.AspNetCore.Identity
@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager


@{
    ViewData["Title"] = "Details";
}

<div class="item-detail-div">
    <div class="item-detail-img-div">
        <img src="@Html.DisplayFor(model => model.PhotoUrl)" class="item-detail-img" />
    </div>
    <div class="item-detail-info-div">
        <div class="item-detail-user-div">
            @if (Model.User.PhotoURL == null)
            {<div class="image-cropper">
                    <img class="item-detail-user-img" src="data:image/png;base64," />
                </div>
            }
            else
            { <div class="image-cropper">
                    <img class="item-detail-user-img" src="@Html.DisplayFor(model => model.User.PhotoURL)" />
                </div>
            }

            <a asp-controller="Profile" asp-action="UserProfile" asp-route-id="@Model.User.Id">
                <h4 class="item-user-username">@Html.DisplayFor(model => model.User.UserName)</h4>
            </a>
        </div>
        <hr />

        <div class="item-detail-item-info-div">
            <h4 class="item-detail-title">@Html.DisplayFor(model => model.Title)</h4><br />
            <div class="rating-photo-div">
                <img class="rating-photo" src="data:image/jpeg;base64," />
                <img class="rating-photo" src="data:image/jpeg;base64," />
                <img class="rating-photo" src="data:image/jpeg;base64," />
                <img class="rating-photo" src="data:image/jpeg;base64," />
            </div>
            <h4 class="item-detail-category-title"> @Html.DisplayNameFor(model => model.Description):</h4><p>@Html.DisplayFor(model => model.Description)</p>
            <a class="item-detail-category-title" target=_blank href="@Html.DisplayFor(model => model.SourceLink)"> Source Link </a><br /><br />
            @if (SignInManager.IsSignedIn(User))
            {
                <a asp-area="" asp-controller="WishListItems" asp-action="Create" asp-route-id="@Model.Id" id="add-wishlist-btn">Add To WishList</a>

            }




        </div>
    </div>
</div>

